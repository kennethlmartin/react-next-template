{
  "name": "react-next-template",
  "version": "1.0.0",
  "author": "Kenneth Martin <developer@kennethmartin.me>",
  "description": "Boilerplate for Next.js projects",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kennethlmartin/react-next-template.git"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:sitemap": "next-sitemap",
    "clean": "npm-run-all -s clean:cache clean:next_build clean:node_modules",
    "clean:cache": "rimraf ./.*cache tsconfig.tsbuildinfo",
    "clean:next_build": "rimraf ./.next",
    "clean:node_modules": "rimraf ./node_modules",
    "lint": "npm-run-all lint:* -p",
    "lint:eslint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check './**/*.js'",
    "lint:stylelint": "stylelint .",
    "lint:typescript": "tsc --noEmit",
    "start": "next start",
    "start:dev": "next dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@emotion/jest": "11.7.1",
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "17.0.8",
    "@types/ramda": "0.27.62",
    "@types/react": "17.0.38",
    "babel-jest": "27.4.6",
    "enzyme-to-json": "3.6.2",
    "eslint-config-archetype": "8.2.0",
    "jest": "27.4.7",
    "next-sitemap": "1.8.4",
    "npm-run-all": "4.1.5",
    "prettier-config-archetype": "2.1.0",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "stylelint-config-archetype": "4.0.0",
    "stylelint-config-prettier": "9.0.3",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@babel/preset-react": "7.16.7",
    "@emotion/babel-plugin": "11.7.2",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@next/bundle-analyzer": "12.0.7",
    "@next/env": "12.0.7",
    "@reduxjs/toolkit": "^1.7.1",
    "next": "12.0.7",
    "next-i18next": "10.1.0",
    "next-redux-wrapper": "7.0.5",
    "next-seo": "4.28.1",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  }
}
