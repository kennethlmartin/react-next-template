{
  "name": "react-next-template",
  "version": "1.0.0",
  "author": "Kenneth Martin <developer@kennethmartin.me>",
  "description": "Boilerplate for Next.js projects",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kennethlmartin/react-next-template.git"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:sitemap": "next-sitemap",
    "clean": "npm-run-all -s clean:cache clean:next_build clean:node_modules",
    "clean:cache": "rimraf ./.*cache",
    "clean:next_build": "rimraf ./.next",
    "clean:node_modules": "rimraf ./node_modules",
    "lint": "npm-run-all lint:* -p",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier --check './**/*.js'",
    "lint:stylelint": "stylelint --syntax css-in-js .",
    "start": "next start",
    "start:dev": "next dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@emotion/babel-plugin": "11.0.0",
    "@emotion/jest": "11.1.0",
    "@next/bundle-analyzer": "10.0.3",
    "@testing-library/dom": "7.28.1",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@testing-library/user-event": "12.2.2",
    "@types/node": "14.14.9",
    "@types/react": "17.0.0",
    "babel-jest": "26.6.3",
    "enzyme-to-json": "3.6.1",
    "eslint-config-archetype": "6.0.0",
    "jest": "26.6.3",
    "next-sitemap": "1.3.6",
    "npm-run-all": "4.1.5",
    "prettier-config-archetype": "1.0.1",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "stylelint-config-archetype": "3.2.1",
    "stylelint-config-prettier": "8.0.2",
    "typescript": "4.1.2"
  },
  "dependencies": {
    "@emotion/react": "11.1.1",
    "@emotion/styled": "11.0.0",
    "@reduxjs/toolkit": "1.4.0",
    "next": "10.0.3",
    "next-redux-wrapper": "6.0.2",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux-saga": "1.1.3"
  }
}
