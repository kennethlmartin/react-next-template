{
  "name": "react-next-template",
  "version": "1.0.0",
  "author": "Kenneth Martin <developer@kennethmartin.me>",
  "description": "Boilerplate for Next.js projects",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kennethlmartin/react-next-template.git"
  },
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:sitemap": "next-sitemap",
    "lint:eslint": "eslint . --cache",
    "lint:prettier": "prettier --check './**/*.js'",
    "lint:stylelint": "stylelint --syntax css-in-js .",
    "start": "next start",
    "start:dev": "next dev",
    "test:lint": "npm-run-all lint:* -p"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@emotion/babel-plugin": "11.0.0",
    "@next/bundle-analyzer": "10.0.1",
    "eslint-config-archetype": "5.2.0",
    "next-sitemap": "1.3.2",
    "npm-run-all": "4.1.5",
    "prettier-config-archetype": "1.0.0",
    "stylelint-config-archetype": "3.2.0",
    "stylelint-config-prettier": "8.0.2"
  },
  "dependencies": {
    "@emotion/react": "11.0.0",
    "@emotion/styled": "11.0.0",
    "@reduxjs/toolkit": "1.4.0",
    "next": "10.0.1",
    "next-redux-wrapper": "6.0.2",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux-saga": "1.1.3"
  }
}
